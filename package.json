{
  "name": "rc-multi-level-selector",
  "version": "0.3.0",
  "description": "A React multilevel selector component",
  "main": "./lib/index.js",
  "scripts": {
    "start-dev": "next dev",
    "build": "babel ./src --out-dir ./lib --copy-files",
    "build-watch": "babel ./src --out-dir ./lib --copy-files --watch",
    "build-docs": "next build",
    "export-docs": "next export",
    "deploy-docs": "npm run build-docs && npm run export-docs && touch out/.nojekyll && push-dir --dir=out --branch=gh-pages --cleanup --verbose --remote=origin",
    "eslint": "standard",
    "stylelint": "stylelint './docs/**/*.jsx'",
    "publish": "npm run build && npm publish",
    "tag": "git tag v$npm_package_version && git push origin --tags",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shianqi/rc-multi-level-selector.git"
  },
  "keywords": [
    "multilevel",
    "selector",
    "react",
    "component"
  ],
  "author": "shianqi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shianqi/rc-multi-level-selector/issues"
  },
  "homepage": "https://github.com/shianqi/rc-multi-level-selector#readme",
  "standard": {
    "parser": "babel-eslint"
  },
  "peerDependencies": {
    "react": "^16.4.x",
    "react-dom": "^16.4.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "@zeit/next-mdx": "^1.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-plugin-transform-define": "^1.3.1",
    "css-loader": "^1.0.0",
    "jest": "^23.6.0",
    "next": "^7.0.2",
    "nova-colors": "^2.1.5",
    "postcss-loader": "^2.1.6",
    "push-dir": "^0.4.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-jss": "^8.6.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.12.1",
    "remark-highlight.js": "^5.0.0",
    "standard": "^11.0.1",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "webpack": "^4.20.2",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {}
}
