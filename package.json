{
  "name": "rc-multi-level-selector",
  "version": "0.1.4",
  "description": "A React multilevel selector component",
  "main": "./lib/index.js",
  "scripts": {
    "start-dev": "next dev",
    "publish": "npm run build && npm publish",
    "tag": "git tag v$npm_package_version && git push origin --tags",
    "build": "babel ./src --out-dir ./lib --copy-files",
    "build-watch": "babel ./src --out-dir ./lib --copy-files --watch",
    "eslint": "eslint --ext .js,jsx ./",
    "test": "jest",
    "test-watch": "jest --watch",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook-deploy": "npm i && npm run build-storybook && storybook-to-ghpages",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shianqi/rc-multi-level-selector.git"
  },
  "keywords": [
    "multilevel",
    "selector",
    "react",
    "component"
  ],
  "author": "shianqi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shianqi/rc-multi-level-selector/issues"
  },
  "homepage": "https://github.com/shianqi/rc-multi-level-selector#readme",
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.3.x",
    "react-dom": "^16.3.x",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "@storybook/addon-actions": "^4.0.0",
    "@storybook/react": "^4.0.0",
    "@storybook/storybook-deployer": "^2.3.0",
    "@zeit/next-mdx": "^1.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "css-loader": "^1.0.0",
    "jest": "^23.6.0",
    "next": "^7.0.2",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "regenerator-runtime": "^0.12.1",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "styled-components": "^4.0.0",
    "webpack": "^4.20.2",
    "webpack-command": "^0.4.1"
